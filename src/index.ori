---
title: Typo.town
---
{{ page.ori(`

    <h1>{{ title }}</h1>

    <h2>Fonts</h2>
    {{ @map/values(fontData, =`
    <a href="fonts/{{ id }}.html">
      <img src="images/{{ id }}.png" alt="{{ name }}" width="400" height="100">
    </a>
    `) }}

    <h2>Tags</h2>
    <ul>
      {{ @map/values(groupedByTag, =`
      <li>
        <a href="tags/{{ @key }}.html">{{ @key }}</a>
      </li>
      `) }}
    </ul>

`) }}

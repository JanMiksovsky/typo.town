---
# Strip the `.html` from the file name to get the tag name.
tagName: !ori (@node/path/basename(@key, ".html"))

# Use the tag name as the page title.
title: !ori tagName
---
{{ page.ori(`

    <h1>{{ tagName }}</h1>

    {{ @map/values(@value, =`
    <a href="../fonts/{{ id }}.html">
      <img src="../images/{{ id }}.png" alt="{{ name }}">
    </a>
    `) }}

`) }}
